{
	"_version": "1.1.0",
	"sap.app": {
		"_version": "1.1.0",
		"id": "lrs.ui5.gestaoContratos",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"___dataSources": {
			"TMS": {
				"uri": "/sap/opu/odata/sap/ZSERVICO_APP/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0"
				}
			}
		}
	},
	"sap.ui": {
		"_version": "1.1.0",
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_bluecrystal",
			"sap_belize",
			"sap_belize_plus"
		]
	},
	"sap.ui5": {
		"_version": "1.1.0",
		"rootView": "lrs.ui5.view.App",
		"dependencies": {
			"minUI5Version": "1.30",
			"libs": {
				"sap.ui.core": {
					
				},
				"sap.m": {
					
				},
				"sap.tnt": {
					
				},
				"sap.ui.layout": {
					
				}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "lrs.ui5.i18n.i18n"
				}
			},
			"side": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/sideContent.json"
			},
			"alerts": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/alerts.json"
			},
			"customer": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/customers.json"
			},
			"listaRfqs": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/mapaComparativoLista.json"
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"controlId": "mainContents",
				"viewType": "XML",
				"controlAggregation": "pages",
				"viewPath": "lrs.ui5.view",
				"async": true
			},
			"routes": [
				{
					"pattern": "",
					"name": "home",
					"target": "home"
				},
				{
					"pattern": "SystemSettings",
					"name": "systemSettings",
					"target": [
						"masterSettings",
						"detailSettings1"
					]
				},
				{
					"pattern": "Tile1Action1",
					"name": "Tile1Action1",
					"target": "Tile1Action1"
				},
				{
					"pattern": "Tile1Action2",
					"name": "Tile1Action2",
					"target": "Tile1Action2"
				},
				{
					"pattern": "MapaComparativoLista",
					"name": "MapaComparativoLista",
					"target": [
						"MapaComparativoLista"
					]
				},
				{
					"pattern" : "MapaComparativoDetalhes/:rfq:",
					"name" : "MapaComparativoDetalhes",
					"target" : "MaraComparativoDetalhes"
				}
			],
			"targets": {
				"home": {
					"viewName": "Home"
				},
				"appSettings": {
					"viewName": "settings.AppSettings"
				},
				"masterSettings": {
					"viewName": "settings.MasterSettings",
					"controlId": "appSettings",
					"controlAggregation": "masterPages",
					"parent": "appSettings"
				},
				"detailSettings1": {
					"viewName": "settings.DetailSettings1",
					"controlId": "appSettings",
					"controlAggregation": "detailPages",
					"parent": "appSettings"
				},
				"Tile1Action1": {
					"viewName": "Tile1Action1"
				},
				"Tile1Action2": {
					"viewName": "Tile1Action2"
				},
				"MapaComparativoLista": {
					"viewName": "MapaComparativoLista"
				},
				"MaraComparativoDetalhes" : {
					"viewName" : "MaraComparativoDetalhes"
				}
			}
		}
	}
}