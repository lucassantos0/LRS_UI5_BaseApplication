<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<Dialog
		contentWidth="768px"
		contentHeight="640px"
		title="{fluxoDocumentos>id}" >
		<beginButton>
			<Button text="Action-A" press="handleCloseButton" />
		</beginButton>
		<endButton>
			<Button text="Action-B" press="handleCloseButton" />
		</endButton>
		<content>
			<Image src="{ProductPicUrl}" width="15em" densityAware="false" />
		</content>
	</Dialog>
</core:FragmentDefinition>