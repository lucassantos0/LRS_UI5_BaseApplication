<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<Dialog
		contentWidth="640px"
		contentHeight="480px">
		<customHeader>
			<Toolbar>
				<ToolbarSpacer />
				<Label text="{fluxoDocumentos>id}" />
				<ToolbarSpacer />
				<Button
					icon="sap-icon://decline"
					onPress="onCloseDetalhesDocumento" />
			</Toolbar>
		</customHeader>
		<content>
			<Image
				src="{ProductPicUrl}"
				width="15em"
				densityAware="false" />
		</content>
	</Dialog>
</core:FragmentDefinition>