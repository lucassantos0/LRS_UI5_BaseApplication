<mvc:View
	controllerName="mapacomparativo.controller.Mapa"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:m="sap.m"
	xmlns="sap.uxap"
	xmlns:layout="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
	xmlns:core="sap.ui.core"
	xmlns:mc="sap.suite.ui.microchart"
	height="100%"
	displayBlock="true">
	<m:App id="app">
		<ObjectPageLayout
			id="mainPagePriceComparison"
			enableLazyLoading="false"
			showTitleInHeaderContent="true"
			showEditHeaderButton="false">
			<headerTitle>
				<ObjectPageHeader
					objectTitle="Mapa comparativo {mcdetails>/rfp}"
					objectSubtitle="{mcdetails>/rfqid}"
					isObjectIconAlwaysVisible="false"
					isObjectTitleAlwaysVisible="false"
					isObjectSubtitleAlwaysVisible="false">
					<actions>
						<ObjectPageHeaderActionButton
							press="HeaderChangeDocument"
							hideText="false"
							icon="sap-icon://search"
							text="Buscar RFP"
							type="Transparent" />
					</actions>
				</ObjectPageHeader>
			</headerTitle>
			<headerContent>
			</headerContent>
			<sections>
				<ObjectPageSection title="Resumo">
					<subSections>
						<ObjectPageSubSection title=" ">
							<blocks>
								<core:Fragment
									fragmentName="mapacomparativo.view.Mapa.ResumoSumario"
									type="XML" />
							</blocks>
						</ObjectPageSubSection>
					</subSections>
				</ObjectPageSection>
				<ObjectPageSection title="Resumo pacote">
					<subSections>
						<ObjectPageSubSection title="Itens geral">
							<blocks>
								<core:Fragment
									fragmentName="mapacomparativo.view.Mapa.ResumoPacote"
									type="XML" />
							</blocks>
						</ObjectPageSubSection>
						<ObjectPageSubSection title="Itens comuns">
							<blocks>
								<core:Fragment
									fragmentName="mapacomparativo.view.Mapa.ItensComuns"
									type="XML" />
							</blocks>
						</ObjectPageSubSection>
						<ObjectPageSubSection
							title="Itens não comuns">
							<blocks>
								<core:Fragment
									fragmentName="mapacomparativo.view.Mapa.ItensNaoComuns"
									type="XML" />
							</blocks>
						</ObjectPageSubSection>
					</subSections>
				</ObjectPageSection>
				<ObjectPageSection title="Análise por item">
					<subSections>
						<ObjectPageSubSection title=" ">
							<blocks>
								<core:Fragment
									fragmentName="mapacomparativo.view.Mapa.AnaliseItem"
									type="XML" />
							</blocks>
						</ObjectPageSubSection>
					</subSections>
				</ObjectPageSection>
				<ObjectPageSection title="Análise contrato (IGPM)">
					<subSections>
						<ObjectPageSubSection title=" ">
							<blocks>
								<core:Fragment
									fragmentName="mapacomparativo.view.Mapa.AnaliseContrato"
									type="XML" />
							</blocks>
						</ObjectPageSubSection>
					</subSections>
				</ObjectPageSection>
			</sections>
		</ObjectPageLayout>
	</m:App>
</mvc:View>